{
  "name": "main-server",
  "version": "1.0.0",
  "main": "dist/main-server/src/app.js",
  "license": "MIT",
  "scripts": {
    "start-prod": "tsc -p tsconfig.prod.json && node dist/main-server/src/app.js",
    "start": "tsc && node dist/main-server/src/app.js",
    "start-demon": "nodemon",
    "test": "tsc && cross-env LOG_LEVEL=warning jest",
    "build": "tsc"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/got": "^9.6.11",
    "@types/jsdom": "^16.1.0",
    "@types/puppeteer": "^3.0.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "got": "^10.6.0",
    "jsdom": "^16.2.0",
    "module-alias": "^2.2.2",
    "node": "^12.0.0",
    "npm": "^6.14.2",
    "puppeteer": "^5.2.1",
    "scryfall-sdk": "^1.6.1",
    "typescript": "^3.8.3",
    "yarn": "^1.21.1"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": "^6.0.0",
    "yarn": "^1.21.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/rewire": "^2.5.28",
    "cross-env": "^7.0.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rewire": "^5.0.0",
    "ts-jest": "^25.2.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
